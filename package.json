{
  "name": "z",
  "version": "0.0.3",
  "main": "src/z.js",
  "description": "pattern matching for javascript",
  "scripts": {
    "test": "node_modules/mocha/bin/mocha tests/*.js",
    "lint": "standard --fix \"src/z.js\" \"tests/*.js\"",
    "coverage": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonardiwagner/z.git..git"
  },
  "author": "Wagner Leonardi",
  "keywords": [
    "pattern",
    "matching",
    "functional",
    "haskell"
  ],
  "standard": {
    "globals": [
      "describe",
      "it",
      "after",
      "afterEach",
      "before",
      "beforeEach"
    ]
  },
  "nyc": {
    "statements": 100,
    "branches": 97.37,
    "functions": 100,
    "lines": 100,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "node_modules/**/*",
      "tests/*"
    ]
  },
  "bugs": {
    "url": "https://github.com/leonardiwagner/z.git./issues"
  },
  "homepage": "https://github.com/leonardiwagner/z.git.#readme",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^3.4.0",
    "mocha": "^2.3.3",
    "nyc": "^10.1.2",
    "standard": "^8.6.0"
  }
}
