{
  "name": "z",
  "version": "1.0.0",
  "main": "src/z.js",
  "description": "pattern matching for javascript",
  "scripts": {
    "test": "NODE_PATH=. mocha src/*.spec.js",
    "test:watch": "npm test -- --watch",
    "test:min": "npm run test:watch -- --reporter min",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "standard --fix \"src/z.js\" \"tests/*.js\"",
    "coverage": "nyc npm test",
    "prettier": "prettier --no-semi --single-quote --trailing-comma es5",
    "format": "npm run prettier -- \"*(src|tests)/**/*.js\" --write",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier -- --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/z-pattern-matching/z"
  },
  "author": "Wagner Leonardi",
  "keywords": [
    "pattern",
    "matching",
    "functional",
    "haskell"
  ],
  "standard": {
    "globals": [
      "describe",
      "it",
      "after",
      "afterEach",
      "before",
      "beforeEach"
    ]
  },
  "nyc": {
    "statements": 100,
    "branches": 97.37,
    "functions": 100,
    "lines": 100,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "node_modules/**/*",
      "tests/*"
    ]
  },
  "bugs": {
    "url": "https://github.com/z-pattern-matching/z/issues"
  },
  "homepage": "https://github.com/z-pattern-matching/z#readme",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^3.4.0",
    "coveralls": "^2.11.16",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.1",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^10.1.2",
    "prettier": "^1.5.2",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "flat": "^2.0.1",
    "install": "^0.10.0",
    "js-function-reflector": "git+https://github.com/leonardiwagner/js-function-reflector.git"
  }
}
